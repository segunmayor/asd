{
  "builds": [
    {
      "src": "asddiagnoses/wsgi.py",
      "use": "@vercel/python",
      "config": {
          "maxLambdaSize": "50mb",
          "maxDuration": 60,
          "runtime": "python3",
          "outputDirectory": "staticfiles_build"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
          "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "asddiagnoses/wsgi.py"
    }
  ]
}
